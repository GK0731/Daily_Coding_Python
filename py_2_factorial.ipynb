   "source": [
    "Write a program which can compute the factorial of a given numbers."
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 1,
   "metadata": {},
   "outputs": [],
   "source": [
    "def fact(num):\n",
    "    if num == 0:\n",
    "        return 1\n",
    "    else:\n",
    "        return num * fact (num - 1)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 2,
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "Enter the no :8\n",
      "40320\n"
     ]
    }
   ],
   "source": [
    "num = input(\"Enter the no :\")\n",
    "num = int(num)\n",
    "result = fact(num)\n",
    "print(result)"
   ]
